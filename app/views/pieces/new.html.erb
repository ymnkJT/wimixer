<%= form_for do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :composer %>
  <%= f.text_field :composer %>
  <br>
  <%= f.label :arranger %>
  <%= f.text_field :arranger %>
  <br>
  <%= f.label :publisher %>
  <%= f.text_field :publisher %>
  <br>
  <%= f.submit %>
<% end %>